<!--index.wxml-->
<view class="container">
  <view class="rank-list">
    <block wx:for="{{ranks}}" wx:key="type">
      <view class="rank-item">
        <navigator class="text" url="/pages/list/list?type={{item.type}}">
          {{item.text}}
        </navigator>
        <view class="arrow"></view>
      </view>
    </block>
      
  </view>
</view>